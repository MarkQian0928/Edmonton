<p id="notice"><%= notice %></p>

<h1>Likes</h1>

<table>
  <thead>
    <tr>
      <th>Like</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @likes.each do |like| %>
      <tr>
        <td><%= like.like %></td>
        <td><%= link_to 'Destroy', like, method: :delete, data: { confirm: 'Are you sure?' } %></td>

      </tr>
    <% end %>

  </tbody>

  </table>


<% @likes.each do |like| %>

  <%= like.id %>
  <% end %>





<br>


